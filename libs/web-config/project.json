{
  "projectType": "library",
  "root": "libs/web-config",
  "sourceRoot": "libs/web-config/src",
  "prefix": "school-v2",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/web-config"],
      "options": {
        "jestConfig": "libs/web-config/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web-config/src/**/*.ts",
          "libs/web-config/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
